{
  "name": "digitalart-contracts",
  "version": "0.1.0",
  "description": "A multi authorship NFT marketplace w/ royalty redistribution and IPR management",
  "type": "commonjs",
  "keywords": [
    "nft",
    "blockchain",
    "ethereum",
    "smart-contracts",
    "media",
    "art",
    "ipr",
    "digital-collectibles",
    "marketplace"
  ],
  "files": [
    "contracts/",
    "scripts/",
    "test/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "start-node": "hardhat node",
    "compile": "hardhat compile",
    "clean": "hardhat clean",
    "test": "hardhat test",
    "flatten": "hardhat flatten",
    "deploy": "hardhat run scripts/digital-art.script.ts --network localhost",
    "lint-contracts": "solhint contracts/**/*.sol",
    "start-server": "tsc && nodemon dist/index.js",
    "commit": "cz",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{scripts,test}/**/*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Innovation-Advisory-Links-Foundation/DigitalArt-contracts.git"
  },
  "author": {
    "name": "LINKS Foundation (BMI-OTB)",
    "email": "giacomo.corrias@linksfoundation.com",
    "url": "https://www.overtheblock.io/"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Innovation-Advisory-Links-Foundation/DigitalArt-contracts/issues"
  },
  "homepage": "https://github.com/Innovation-Advisory-Links-Foundation/DigitalArt-contracts#readme",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.1.0",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "chai": "^4.3.4",
    "commitizen": "^4.2.4",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.3.1",
    "hardhat": "^2.4.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "solhint": "^3.3.6",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  },
  "config": {
    "solidity": {
      "version": "0.8.0",
      "settings": {
        "outputSelection": {
          "contracts/DigitalArt.sol": {
            "DigitalArt": [
              "devdoc"
            ]
          }
        }
      }
    },
    "paths": {
      "contracts": "./contracts",
      "tests": "./test",
      "cache": "./cache",
      "build": {
        "contracts": "./artifacts/contracts"
      }
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@google-cloud/vision": "^2.3.7",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/request": "^2.48.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "request": "^2.88.2"
  }
}
